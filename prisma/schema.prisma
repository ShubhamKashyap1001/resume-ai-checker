
generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model Resume {
  id              Int      @id @default(autoincrement())
  resumeText      String   @db.Text
  jobDescription  String   @db.Text
  skillMatch      Float
  experienceScore Float
  atsScore        Float
  grammarScore    Float
  ruleScore       Float
  finalScore      Float
  rating          String
  createdAt       DateTime @default(now())
}