generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Resume {
  id                     Int      @id @default(autoincrement())
  resumeText             String
  jobDescription          String

  skillMatch              Float
  experienceScore         Float
  atsScore                Float
  grammarScore            Float
  ruleScore               Int
  finalScore              Float
  rating                  String

  strengths               String[]
  weaknesses              String[]
  missingSkills            String[]
  improvementSuggestions  String[]
  finalVerdict             String?

  createdAt               DateTime @default(now())
}
